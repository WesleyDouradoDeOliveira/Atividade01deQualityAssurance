package exemplos;

import java.util.Scanner;

public class CustoCarro {
	
		//10) o custo de um carro novo ao consumidor é a soma do custo de fabrica com a porcentagem do distribuidor e dos impostos
		//(aplicados ao custo de fabrica).supondo que o percentual do distribuidor seja de 28% e os impostos de 45%. escrever um algoritmo
	    //para ler o custo de fabrica de um carro, calcular e escrever o custo final ao consumidor
	
		public static void main(String [] args) {
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite o custo de fabricação do carro: ");
		double fabricacao=sc.nextDouble();
		
		//definindo as porcentagens %%%%%
		double distribuidor = 0.28;
		double impostos = 0.45;
		
		
        // Calcular o custo do distribuidor
        double custoDistribuidor = fabricacao * distribuidor;

        // Calcular o custo dos impostos
        double impostos1 = fabricacao * impostos;

        // Calcular o custo final ao consumidor
        double custoFinal = fabricacao + custoDistribuidor + impostos1;
		
		System.out.println("O custo do carro de fabrica é : "+custoFinal);
		
		sc.close();
	}

}






package exemplos;

public class CarroCaneta {

    public static void main(String[] args) {
        // Criar uma instância de Carro
        Carro meuCarro = new Carro();
        
        // Exibir o estado inicial do carro
        meuCarro.estado();
        
        // Ligar o carro
        meuCarro.ligar();
        
        // Acelerar o carro
        meuCarro.acelerar();
        
        // Exibir o estado do carro após ligar e acelerar
        meuCarro.estado();
        
        // Criar uma instância de Caneta
        Caneta minhaCaneta = new Caneta("Azul", 0.7);
        
        // Exibir o estado inicial da caneta
        minhaCaneta.estado();
        
        // Destampar a caneta
        minhaCaneta.destampar();
        
        // Escrever com a caneta
        minhaCaneta.escrever();
        
        // Exibir o estado da caneta após destampar e escrever
        minhaCaneta.estado();
    }
}







