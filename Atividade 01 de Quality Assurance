package Exemploss;
import java.util.Scanner;
public class SomaEnquanto {
public static void main(String[] args) {
Scanner leitura = new Scanner (System.in);
//apresentar o total da soma obtida dos cem primeiros numeros inteiros (1+2+3+4+...+98+99+100).

	int soma=0 ;
	int contadora=1;
	while(contadora<101) {
	soma = soma+contadora;
	contadora++;
	System.out.println("A soma dos 100 primeiros numeros possitivos é :"+ soma);
	
	
}
leitura.close();	
}
}






package Exemplossss;

public class TryCatch {
	public static void main(String[] args) {
		try {
			int data = 100/0;
			System.out.println(data);
		}catch(Exception e) {
			System.out.println("Não é possivel efetuar a divisão por 0");
			
		}
	}

}







package Exemplossss;

public class Temporizador {
	public static void main(String[] args) throws InterruptedException {
		
		for (int i = 0; i < 60; i++) {
			for(int j = 0;j<60;j++) {
				System.out.println(i+":"+j);
				Thread.sleep(2000);
				
			}
		}
	}

}







package Exemplossss;

import java.util.Scanner;

public class Tempoorizador {
	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		
		System.out.println("Digite o valor dos minutos");
		int minutos = sc.nextInt();
		
		System.out.println("Digite o valor dos segundos");
		int segundos = sc.nextInt();
		
		try {
		for(int i = minutos; i >=0;i--) {
			for(int j =segundos; j >=0; j--) {
				System.out.println(i+":"+j);
				Thread.sleep(1000);
			}
			segundos=59;
		}
		System.out.println("BOOMMMM");
	}catch(Exception e) {
		System.out.println("Ele nao esperou 1 segundo");
	}
		sc.close();
}
}







package Exemplossss;

import java.util.Scanner;

public class ExemploWhileDC {

public static void main(String[] args) {

Scanner sc = new Scanner (System.in);

int x=1, r=0;
String resp;
resp="sim";

while (resp.equals("sim")) {
	System.out.println("Digite um numero:");
	x=sc.nextInt();
	r= x*3;
	System.out.println("A resposta da multiplicação é :"+r);
	System.out.println("Deseja efetuar outra operação ? :");
	resp=sc.next();
	System.out.println(resp);
}
sc.close();	

}
}










package Exemplossss;

public class ExemplosDoWhille {
	public static void main(String[] args) {
		int contadora = 0;
		
		do {
			System.out.println(contadora);
			contadora++;
			
		} while (contadora<9);
	}

}












package Exemplossss;
import java.util.Scanner;
public class ExemploDoWhileDC {
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);
		
int x,r;		
String resp;
resp="sim";
do {
	System.out.println("Digite um valor para ser multiplicado");
	x=sc.nextInt();
	r=x*3;
	System.out.println("A resposta da multiplicação é :"+r);
	System.out.println("Deseja continuar ?");
	resp=sc.next();
	
}while(resp.equals("sim"));
		System.out.println("Encerrar Programa");
		
		
		
		
		
		
		
		
sc.close();	
}
}










package Exemplossss;

public class Cronoometro {
	public static void main(String[] args) {
		try {
			for (int i = 0; i < 60; i++) {
				for (int j = 0; i < 60; i++) {
						System.out.println(i + "i" +j);
						Thread.sleep(1000);
					}
				}
			}catch(Exception e) {
				System.out.println("Não foi possivel adicionar um segundo em casa");
		}
	}

}






package Exemplossss;

public class Cronometro {
	public static void main(String[] args) {
		
		for (int i = 0; i < 60; i++) {
			for(int j = 0;j<60;j++) {
				System.out.println(i+":"+j);
			}
		}
	}

}





