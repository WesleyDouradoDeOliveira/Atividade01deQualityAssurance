package Exemploss;
import java.util.Scanner;
public class EnquantoTabuada {
public static void main(String[] args) {
Scanner sc = new Scanner(System.in);	

	System.out.println("Digite um numero que deseja saber a sua tabuada :");
	int numero=sc.nextInt();
	int contadora=1;
	while(contadora<11) {
		System.out.println(numero+"x"+contadora+"="+numero*contadora);
		contadora++;	
}
	
	
	
	
sc.close();
}
}






package Exemploss;
public class MostrarImpares {
public static void main(String[] args) {
	
	
	int contadora=1;
	
	System.out.println("Os numeros impares entre (0 e 20) é ");
	
	while(contadora<21) {
		
		if(contadora%2==1) {
			System.out.println(contadora);
		}
		
		contadora++;


}
}
}










