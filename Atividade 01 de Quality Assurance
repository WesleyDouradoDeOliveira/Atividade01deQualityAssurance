package Exxxxxxxxxxxxxxxxxxxxxxxxxxx;

public interface Controle {
    void ligar();

    void desligar();

    void aumentarVolume();

    void diminuirVolume();
    
    void mudar(double x);
}






package Exxxxxxxxxxxxxxxxxxxxxxxxxxx;

		public class ControleApp {
		public static void main(String[] args) {
		ControleTV ctv = new ControleTV();
        ControleRadio cr = new ControleRadio();
        
        System.out.println("Dados do controle da tv");
        System.out.println("TV esta ligada? " +ctv.getLigado());
        System.out.println("Canal: "+ctv.getCanal());
        System.out.println("Volume: " +ctv.getVol());
        
        System.out.println("\n\nDados do controle do Radio");
        System.out.println("Radio esta ligado? "+cr.getLigado());
        System.out.println("Estacao: "+cr.getEstacao());
        System.out.println("Volume: "+cr.getVol());


    }
}








package Exxxxxxxxxxxxxxxxxxxxxxxxxxx;

public class ControleRadio implements Controle {
    private boolean ligado;
    private int vol;
    private double estacao;

    public void ligar() {
        if (this.ligado == true)
            System.out.println("Erro! Rádio já está ligado");
        else
            this.ligado = true;
    }

    public boolean getLigado() {
        return ligado;
    }

    public int getVol() {
        return vol;
    }

    public double getEstacao() {
        return estacao;
    }

    public void desligar() {
        if (this.ligado == true) {
            this.ligado = false;
            this.vol = 0;
            this.estacao = 0;
        } else
            System.out.println("Erro! Rádio já está desligado");
    }

    public void aumentarVolume() {
        if (this.ligado == true && this.vol < 100)
            this.vol++;
        else
            System.out.println("Erro! Verifique se está ligado ou se o volume está no máximo");
    }

    public void diminuirVolume() {
        if (this.ligado == true && this.vol > 0)
            this.vol--;
        else
            System.out.println("Erro! Rádio já está ligado");
    }

    public void mudar(double x) {
        // Implemente a lógica para mudar a estação
    }
}








package Exxxxxxxxxxxxxxxxxxxxxxxxxxx;

public class ControleTV implements Controle {
    private boolean ligado;
    private int vol = 5;
    private int canal;

    public void ligar() {
        if (this.ligado == true)
            System.out.println("Erro! A TV já está ligada");
        else
            this.ligado = true;
    }

    public boolean getLigado() {
        return ligado;
    }

    public int getVol() {
        return vol;
    }

    public int getCanal() {
        return canal;
    }

    public void desligar() {
        if (this.ligado == true) {
            this.ligado = false;
            this.vol = 0;
            this.canal = 0;
        } else
            System.out.println("Erro! A TV já está desligada");
    }

    public void aumentarVolume() {
        if (this.ligado == true && this.vol < 100)
            this.vol++;
        else
            System.out.println("Erro! Verifique se a TV está ligada ou se o volume está no limite");
    }

    public void diminuirVolume() {
        if (this.ligado == true && this.vol > 0)
            this.vol--;
        else
            System.out.println("Erro! Verifique se a TV está ligada ou se o volume está no mínimo");
    }

    public void mudar(double x) {
        // Implemente a lógica para mudar de canal ou estação
    }
}









package Heranca;

public class Poupanca extends ContaCorrente{
	
	public void sacar(double valor)
	{
		this.saldo=this.saldo-valor;
	}

}





package Heranca;

public class ContaEspecial extends ContaCorrente{
	
	public void sacar(double valor)
	{
		double taxa=valor*0.001;
		this.saldo=this.saldo-valor-taxa;
	}

}






package Heranca;

public class ContaCorrente {
	private String nome;
	private String numConta;
	protected double saldo=0;
	
	public void depositar(double valor)
	{
		this.saldo=this.saldo+valor;
	}
	public double verSaldo()
	{
		return this.saldo;
		
	}
	public void sacar (double valor)
	{
		double taxa=valor*0.05;
		this.saldo=this.saldo-valor-taxa;
	}
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	public String getNumConta() {
		return numConta;
	}
	public void setNumConta(String numConta) {
		this.numConta = numConta;
	}

}




package Polimorfismo;

public class ExemploSobrecarga {
	public static void main(String[] args) {
		Sobrecarga s = new Sobrecarga();
		Sobrecarga s1 = new Sobrecarga(55.7);
		System.out.println("O valor de s é: "+s.getValor());
		System.out.println("O valor de s1 é: "+s1.getValor());
		s.soma();
		System.out.println("A soma é: " +s.getValor());
		s.soma(3.5);
		System.out.println("A soma é: "+s.getValor());
		s.soma(666);
		System.out.println("A soma é: "+s.getValor());
		s.soma(55.4, 22);
		System.out.println("A soma é: "+s.getValor());
		s.soma(12, 50.4);
		System.out.println("A soma é: "+s.getValor());
		s.soma(19, 5.0);
		System.out.println("A soma é: "+s.getValor());
		s.soma(33, 22, 11);
		System.out.println("A soma é: "+s.getValor());
	}
}







package Polimorfismo;

public class Sobrecarga {
	
	private double valor=0.0;
	
	public Sobrecarga()
	{
		this.valor=15;
	}
	public Sobrecarga(double n1)
	{
		this.valor=23+n1;
	}
	
	public void soma()
	{
		this.valor=5+3;
	}
	public void soma(int n1)
	{
		this.valor=5.0+n1;
	}
	public void soma(double n1)
	{
		this.valor=5+n1;
	}
	public void doms(int n1, int n2)
	{
		this.valor=n1+n2;
	}
	public void soma(int n1, double n2)
	{
		this.valor=n1+n2;
	}
	public void soma(double n1, int n2)
	{
		this.valor=n1+n2;
	}
	public void soma(int n1, int n2, int n3)
	{
		this.valor= n1 + n2 + n3;
	}
	public double getValor() {
		return valor;
	}
}






package Tabuada;

import java.util.Scanner;

public class Tabuada {

public static void main(String[] args) {
Scanner sc=new Scanner(System.in);
	
System.out.println("Digite um numero para saber a tabuada:");
int numero=sc.nextInt();
int contadora=1;
while(contadora<11) {
System.out.println(numero+ " X " +contadora + " = " + numero*contadora);
contadora++;



sc.close();
}
}
}








































































































































































































