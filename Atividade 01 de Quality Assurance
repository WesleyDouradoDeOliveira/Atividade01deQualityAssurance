package exemplos;
;

public class Somas {
	
		public static void main(String [] args) {
		
		//Entrada de Dados
		int a= 6 * (3+2);       //Processamento
		int b= 2+(6*(3+2)) ;          //Processamento
		int c= 2+(3*6)/(2+4) ;        //Processamento
		int d= 2*(8/(3+1))   ;      //Processamento
		int e= 3+(16-2)/(2*(9-2)) ;        //Processamento
		int f= (6/3)+(8/2)   ;      //Processamento
		int g= ((3+(8/2))*4)+(3*2)  ;       //Processamento
		int h= (6*(3*3)+6)-10  ;       //Processamento
		int i=  (((10*8)+3)*9)  ;      //Processamento
		int j=  ((-12)*(-4))+(3*(-4)) ;       //Processamento
		
		//Saida
		System.out.println(a);
		System.out.println(b);
		System.out.println(c);
		System.out.println(d);
		System.out.println(e);
		System.out.println(f);
		System.out.println(g);
		System.out.println(h);
		System.out.println(i);
		System.out.println(j);
		
	}

}







