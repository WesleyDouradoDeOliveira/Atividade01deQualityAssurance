package Encapsulamento;
public interface InterRadio {
	
	public abstract void ligar ();
	public abstract void desligar();
	public abstract void maisvolume();
	public abstract void menosvolume();
	public abstract void trocarestacao();
	public abstract void mudo();
	


}









package Encapsulamento;

public interface InterRelogio {
	
	public abstract void ajustarHora(int h);
	public abstract void ajustarMinuto(int m);
	public abstract void mostrarHora( );
	public abstract void ligarAlarme(int h, int m);
	public abstract void desligarAlarme();

}








	package Encapsulamento;
	public class Radio implements InterRadio{
	//ATRIBUTOS
	private int vol;
	private Boolean ligado;
	
	
	public int getVol() {
		return vol;
	}
	public Boolean getLigado() {
		return ligado;
	}
		////////////////////////////////////////////////////
		public void ligar() {
		if(this.ligado==true)
			System.out.println("O radio ja esta ligado");
		else {
			this.ligado=true;
			this.vol=5;
		}
	}
	/////////////////////////////////////////////////////
	public void desligar() {
		if(this.ligado==false)
			System.out.println("O radio ja esta desligado");
		else 
			this.ligado=false;
	
		
	}
	@Override
	public void maisvolume() {
		// TODO Auto-generated method stub
		
	}
	@Override
	public void menosvolume() {
		// TODO Auto-generated method stub
		
	}
    /////////////////////////////////////////////////////
	public void trocarestacao() {
		if(this.ligado==false)
			System.out.println("Trocando a estação...");
		else 
			System.out.println("O radio esta desligado");
	}
	/////////////////////////////////////////////////////
	
	public void mudo() {
		this.vol=0;
	/////////////////////////////////////////////////////	
	}

}











package Encapsulamento;

public class RadioRelogio implements InterRadio, InterRelogio {
    private int vol;
    private int hora;
    private int minuto;
    @Override
    public void ajustarHora(int h) {
        if (h >= 0 && h < 24) {
            this.hora = h;
        } else {
            System.out.println("Hora inválida");
        }
    }

    @Override
    public void ajustarMinuto(int m) {
        if (m >= 0 && m < 60) {
            this.minuto = m;
        } else {
            System.out.println("Minuto inválido");
        }
    }

    @Override
    public void mostrarHora() {
        System.out.println(this.hora + ":" + this.minuto);
    }

    @Override
    public void ligarAlarme(int h, int m) {
        System.out.println("Alarme ligado para " + h + ":" + m);
    }

    @Override
    public void desligarAlarme() {
        System.out.println("Alarme desligado");
    }

    @Override
    public void ligar() {
        System.out.println("O rádio está ligado");
    }

    @Override
    public void desligar() {
        System.out.println("O rádio está desligado");
    }

    @Override
    public void maisvolume() {
        if (this.vol < 10) {
            this.vol++;
            System.out.println("Volume aumentado para " + this.vol);
        } else {
            System.out.println("Volume no máximo");
        }
    }

    @Override
    public void menosvolume() {
        if (this.vol > 0) {
            this.vol--;
            System.out.println("Volume diminuído para " + this.vol);
        } else {
            System.out.println("Volume no mínimo");
        }
    }

    @Override
    public void trocarestacao() {
        // Implementação para trocar de estação de rádio
        System.out.println("Estação de rádio trocada");
    }

    @Override
    public void mudo() {
        this.vol = 0;
        System.out.println("Som mudo");
    }
}









package Encapsulamento;

public class Relogio implements InterRelogio{

	private int hora;
	private int minuto;
	private int horaAlarme;
	private int minutosAlarme;
	
	
	@Override
	public void ajustarHora(int h) {
		if(h>0 && h<=12)
			this.hora=h;
		else
			System.out.println("Valor invalido...");
	}
	
	
	@Override
	public void ajustarMinuto(int m) {
		if(m>=0 && m<60)
			this.minuto=m;
		else
			System.out.println("Valor invalido...");
	}
	
	
	@Override
	public void mostrarHora() {
		System.out.println("São " +this.hora+" horas e "+this.minuto+" minutos");
	}
	
	
	@Override
	public void ligarAlarme(int h, int m) {
		this.horaAlarme=h;
		this.minutosAlarme=m;
		
	}
	@Override
	public void desligarAlarme() {
		System.out.println("Alarme desligado");
		
	}


	public int getHoraAlarme() {
		return horaAlarme;
	}


	public int getMinutosAlarme() {
		return minutosAlarme;
	}
	
}







